{
  "name": "node-canvas-text",
  "version": "1.0.2",
  "description": "Using a provided Opentype.js font object, draws a string, sized to fit inside a given rectangle on an HTML5 canvas",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaivi/node-canvas-text.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "tsc -p tsconfig.json",
    "browserify": "browserify src/node-canvas-text/browsertest.ts -p [ tsify -p tsconfig.json ] -o dist/browser.js",
    "start": "budo src/node-canvas-text/browsertest.ts:dist/browser.js --live -- -p [ tsify -p tsconfig.json ]"
  },
  "keywords": [
    "canvas",
    "text",
    "fit",
    "squeeze",
    "rectangle",
    "canvas2d"
  ],
  "author": "Kai Vik",
  "license": "ISC",
  "devDependencies": {
    "@types/hyphen": "^1.6.7",
    "@types/node": "^20.10.6",
    "@types/opentype.js": "^1.3.8",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^17.0.0",
    "budo": "^11.8.4",
    "canvas": "^2.11.2",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.6.0",
    "keycode": "^2.2.1",
    "ts-loader": "^9.5.1",
    "tsify": "^5.0.4",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "hyphen": "^1.10.4",
    "linebreak": "^1.1.0",
    "node-fs": "^0.1.7",
    "opentype.js": "^1.3.4",
    "tex-linebreak": "0.6.0"
  }
}
